---
import BaseHead from "../components/BaseHead.astro";
import { SITE_TITLE, SITE_DESCRIPTION } from "../consts";
import Header from "../components/Header.astro";
import { Content as ResumeContent } from "../content/general/resume.md";
---

<BaseHead title={SITE_TITLE} description={SITE_DESCRIPTION} />
<Header />
<div class="flex flex-row w-screen justify-center content-start">
  <article class="prose prose-xl prose-h1:underline self-center max-w-4xl p-4">
    <ResumeContent />
  </article>
</div>
